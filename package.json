{
  "name": "nixos-kotlin-dev-env",
  "version": "1.0.0",
  "type": "module",
  "keywords": [
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "slidev build slides.md",
    "dev": "slidev slides.md --open --inspect",
    "export": "slidev export slides.md",
    "format": "prettier --config=.prettierrc.json5 --write .",
    "screenshot": "slidev export slides.md --format png",
    "start": "slidev slides.md --open"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/noto-sans": "^5.2.10",
    "@iconify-json/vscode-icons": "^1.2.41",
    "@slidev/types": "^52.12.0"
  },
  "devDependencies": {
    "@slidev/cli": "^52.12.0",
    "prettier": "3.8.0",
    "prettier-plugin-slidev": "1.0.5"
  },
  "slidev": {
    "colorSchema": "both",
    "aspectRatio": "16:9",
    "canvasWidth": 1080
  }
}
